<!DOCTYPE html>
<html lang="en">
<head>
  <title>Javascript Store Homepage</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="/js/cart.js"></script>
  <link href="/css/style.css" rel="stylesheet">
</head>
<body>

<!-- Menu -->
<nav class="navbar navbar-expand-sm fixed-top px-4 bg-white">
  <div class="container-fluid">
    <a class="navbar-brand" href="/">
      <img src="../img/logo-fruit-01.png" class="mx-auto d-block">
    </a>
    <div class="collapse navbar-collapse" id="mynavbar">
      <ul class="navbar-nav me-auto">
      </ul>
      <div class="d-flex">
          <ul class="navbar-nav me-auto">
              <li class="nav-item">
                <a href="account">
                  <img src="../img/user.png" width="50" class="mx-auto d-block px-1">
                </a>
              </li>
              <li class="nav-item">
                <a href="cart">
                  <img src="../img/shopping-cart.png" width="50" class="mx-auto d-block px-1">
                </a>
              </li>
          </ul>
      </div>
    </div>
  </div>
</nav>


<!-- Page title -->
<div class="container-fluid p-5 text-center" style="margin-top: 150px;">
  <h1>Your cart</h1>
</div>
  

<!-- Container -->
<div class="container mt-5">
    <div class="row">

      <table class="table">
        <thead>
          <tr>
            <th scope="col">Item</th>
            <th scope="col">Product ID</th>
            <th scope="col">Product Name</th>
            <th scope="col">Product Price</th>
            <th scope="col">Product Quantity</th>
          </tr>
        </thead>
        <tbody class="cart-table">
          
        </tbody>
      </table>
      
      <button type="button" class="checkout btn btn-outline-success mx-auto d-block">Checkout</button>
    
    </div> 
</div>

</body>
</html>
